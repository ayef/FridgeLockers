	        android:entryValues="@array/pref_bannedTimeEnd_values"
	        android:defaultValue="@string/pref_bannedTimeEnd_default" />
	    <com.sensorcon.ListPreferenceShowSummary
	        android:key="pref_snsrdrnPollInterval"
	        android:title="@string/pref_snsrdrnPollInterval"
