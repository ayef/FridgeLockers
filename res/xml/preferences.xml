<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
	    <CheckBoxPreference
	        android:key="pref_alarm"
	        android:title="@string/pref_alarm"
	        android:defaultValue="true" />
	    <CheckBoxPreference
	        android:key="pref_tweet"
	        android:title="@string/pref_tweet"
	        android:defaultValue="true" />
	    <CheckBoxPreference
	        android:key="pref_postFB"
	        android:title="@string/pref_postFB"
	        android:defaultValue="true" />
	    <CheckBoxPreference
	        android:key="pref_collectStats"
	        android:title="@string/pref_collectStats"
	        android:defaultValue="true" />
	    <com.sensorcon.ListPreferenceShowSummary
	        android:key="pref_bannedTimeStart"
	        android:title="@string/pref_bannedTimeStart"
